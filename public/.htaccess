php_value include_path ".:/usr/local/apache2/htdocs/lib/ZendFramework/library:/usr/local/apache2/htdocs/lib/zf2/library"

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ index.php [NC,L]
